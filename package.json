{
  "name": "easyiolabs-electronics",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run clean && cross-env NEXT_PHASE=phase-production-build next build",
    "build:static": "cross-env NEXT_PHASE=phase-production-build next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf .next && rimraf out && rimraf node_modules/.cache",
    "clean:all": "rimraf .next && rimraf out && rimraf node_modules && npm install",
    "build:clean": "npm run clean && npm run build",
    "build:netlify": "cross-env NEXT_PHASE=phase-production-build next build",
    "build:vercel": "cross-env NEXT_PHASE=phase-production-build next build",
    "migrate": "npx tsx scripts/migrate-content.ts",
    "test-db": "npx tsx scripts/test-database.ts",
    "setup": "echo 'Run: psql -U postgres -d easyiolabs_cms -f database/setup.sql'"
  },
  "dependencies": {
    "@types/react-syntax-highlighter": "^15.5.11",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.5.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "next": "14.0.4",
    "pg": "^8.11.3",
    "quill": "^1.3.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-quill": "^2.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.4.32",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
